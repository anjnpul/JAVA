<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ViewData</servlet-name>
  	<display-name>ViewData</display-name>
  	<description></description>
  	<servlet-class>controller.data.ViewData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DataCrud</servlet-name>
  	<display-name>DataCrud</display-name>
  	<description></description>
  	<servlet-class>controller.data.DataCrud</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<display-name>Login</display-name>
  	<description></description>
  	<servlet-class>controller.user.Login</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ViewData</servlet-name>
  	<url-pattern>/ViewData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DataCrud</servlet-name>
  	<url-pattern>/DataCrud</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/Login</url-pattern>
  </servlet-mapping>
</web-app>
